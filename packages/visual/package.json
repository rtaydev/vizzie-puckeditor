{
	"name": "@puck-editor/visual",
	"version": "0.1.0",
	"private": false,
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./styles.css": {
			"default": "./dist/styles.css"
		},
		"./index.css": {
			"default": "./dist/index.css"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": [
		"**/*.css"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint": "eslint ."
	},
	"peerDependencies": {
		"@measured/puck": "^0.20.2",
		"react": ">=18",
		"react-dom": ">=18"
	},
	"devDependencies": {
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"postcss": "^8.5.6",
		"postcss-modules": "^6.0.1",
		"tsup": "^8.0.0",
		"typescript": "^5.0.0"
	}
}
